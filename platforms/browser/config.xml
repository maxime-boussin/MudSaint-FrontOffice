<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.mboussin.mudsaint" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mud Saint</name>
    <description>
        A game designed by Maxime Boussin
    </description>
    <author email="maxime.boussin35@gmail.com" href="http://cordova.io">
        Maxime Boussin
    </author>
    <icon src="www/img/icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="UIWebViewBounce" value="false" />
</widget>
